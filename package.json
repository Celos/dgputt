  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^26.0.24",
    "@types/uuid": "^8.3.4",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
